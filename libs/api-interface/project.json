{
    "name": "api-interface",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/api-interface/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/api-interface"
            }
        },
        "test": {
            "executor": "@nx/vite:test",
            "outputs": ["coverage/libs/api-interface"],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/libs/api-interface"
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/api-interface/**/*.ts", "libs/api-interface/package.json"]
            }
        }
    },
    "tags": []
}
